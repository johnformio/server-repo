{
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "PROTOCOL=http DOMAIN=localhost PORT=3000 node main.js",
    "test": "FORMIO_HOSTED=true TEST_SUITE=1 PROTOCOL=http DOMAIN=localhost PORT=3000 MONGO_COLLECTION=formio-test PROJECT_PLAN=trial DROPBOX_CLIENTID=clientid123 DROPBOX_CLIENTSECRET=clientsecret123 mocha -b -t 30000 test/formio.js --exit",
    "docker": "FORMIO_HOSTED=true DOCKER=1 TEST_SUITE=1 PROTOCOL=http DOMAIN=localhost PORT=3000 DROPBOX_CLIENTID=NOTREAL DROPBOX_CLIENTSECRET=clientsecret123 mocha -b -t 30000 test/docker.js",
    "customer": "TEST_SUITE=1 PROTOCOL=http DOMAIN=localhost PORT=3000 DROPBOX_CLIENTID=NOTREAL DROPBOX_CLIENTSECRET=clientsecret123 mocha -b -t 30000 test/customer.js",
    "clean": "rm -rf build",
    "compile": "node compile",
    "dist": "pkg .; cp node_modules/opn/xdg-open .",
    "build": "npm run lint; npm run clean; npm run compile",
    "build:debug": "docker build -t formio-server-debug -f deployment/docker/formio-server/Dockerfile-debug .",
    "package:debug": "docker save -o formio-server-debug formio-server-debug",
    "builddocker": "docker build --squash -f deployment/docker/formio-server/Dockerfile .",
    "lint": "eslint .",
    "validate": "circleci config validate -c .circleci/config.yml"
  },
  "main": "./server.js",
  "schema": "3.3.0",
  "portalVersion": "6.1.3",
  "version": "6.2.3",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "adal-node": "^0.1.28",
    "async": "^2.6.0",
    "aws-sdk": "^2.395.0",
    "azure-storage": "^2.10.2",
    "body-parser": "^1.18.2",
    "chance": "^1.0.18",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-cache-controller": "^1.1.0",
    "formio": "^1.40.0",
    "formio-services": "formio/formio-services#v2.0.0",
    "formio-workers": "^1.9.0",
    "formiojs": "^3.13.6",
    "is-url": "^1.2.4",
    "jira-connector": "^2.10.0",
    "jslogger": "^1.0.3",
    "jsonwebtoken": "^8.4.0",
    "keygenerator": "^1.0.4",
    "kickbox": "^2.0.4",
    "ldapauth-fork": "^4.2.0",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "minio": "^7.0.5",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "multer": "^1.4.1",
    "node-hubspot": "https://github.com/formio/node-hubspot.git",
    "node-paginate-anything": "^1.0.0",
    "on-finished": "^2.3.0",
    "q": "^1.5.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "resourcejs": "^1.33.0",
    "restler": "^3.4.0",
    "semver": "^5.6.0",
    "serve-favicon": "^2.5.0",
    "squel": "^5.12.2",
    "through": "^2.3.8",
    "tldjs": "^2.3.1",
    "traverse": "^0.6.6",
    "twilio": "^3.27.1",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-formio": "^1.1.2",
    "mocha": "^5.1.0",
    "pre-commit": "^1.2.2",
    "prepack": "^0.2.54",
    "sinon": "^7.2.3",
    "supertest": "^3.4.2",
    "uglify-es": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^4.29.0"
  },
  "pre-commit": [
    "lint"
  ],
  "bin": "main.js",
  "name": "formio",
  "pkg": {
    "assets": [
      "package.json",
      "package-lock.json",
      "favicon.ico",
      "portal/**/*",
      "node_modules/formio/src/db/**/*",
      "node_modules/threads/lib/worker.node/slave.js"
    ],
    "targets": [
      "node8-alpine-x64"
    ]
  }
}
