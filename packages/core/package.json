    "typedoc": "^0.22.11",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@formio/lodash": "0.0.8",
    "core-js": "^3.21.0",
    "dayjs": "^1.10.7",
    "dompurify": "^2.3.5",
    "eventemitter3": "^4.0.7",
    "fetch-ponyfill": "^7.1.0",
    "json-logic-js": "^2.0.2"
  }
