    "gulp-template": "^5.0.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.4.0",
    "mocha-jsdom": "^2.0.0",
    "mock-local-storage": "^1.1.20",
    "power-assert": "^1.6.1",
    "sinon": "^17.0.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.1.2",
