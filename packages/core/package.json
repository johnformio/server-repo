{
  "name": "@formio/core",
  "version": "0.0.9",
  "description": "The core Form.io renderering framework.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register --r mock-local-storage '**/*.spec.ts'",
    "docs": "./node_modules/typedoc/bin/typedoc --exclude '*.spec.ts' src/*.ts src/**/*.ts src/**/**/*.ts",
    "build:core": "webpack --config config/webpack.core.js && webpack --config config/webpack.core.prod.js",
    "build:formio": "webpack --config config/webpack.formio.js && webpack --config config/webpack.formio.prod.js",
    "build:form": "webpack --config config/webpack.form.js && webpack --config config/webpack.form.prod.js",
    "build:validator": "webpack --config config/webpack.validator.js && webpack --config config/webpack.validator.prod.js",
    "build": "gulp templates && tsc && npm run docs && npm run build:core && npm run build:formio && npm run build:form && npm run build:validator",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/core.git"
  },
  "keywords": [
    "The",
    "core",
    "Form.io",
    "rendering",
    "framework."
  ],
  "author": "Travis Tidwell <travis@form.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formio/core/issues"
  },
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/formio/core#readme",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/chance": "^1.1.1",
    "@types/dompurify": "^2.2.1",
    "@types/fetch-mock": "^7.3.3",
    "@types/lodash": "^4.14.168",
    "@types/lodash.template": "^4.5.0",
    "@types/mocha": "^8.2.1",
    "@types/power-assert": "^1.5.3",
    "@types/sinon": "^9.0.11",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.4",
    "chance": "^1.1.7",
    "fetch-mock": "^9.11.0",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^5.0.0",
    "lodash": "^4.17.21",
    "mocha": "^8.3.2",
    "mocha-jsdom": "^2.0.0",
    "mock-local-storage": "^1.1.17",
    "power-assert": "^1.6.1",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@formio/lodash": "0.0.4",
    "dayjs": "^1.10.4",
    "deepmerge": "^4.2.2",
    "dompurify": "^2.2.7",
    "eventemitter3": "^4.0.7",
    "fetch-ponyfill": "^7.1.0",
    "json-logic-js": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
