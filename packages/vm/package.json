{
    "name": "@formio/vm",
    "version": "0.0.5",
    "main": "build/index.js",
    "license": "OSL-3.0",
    "files": [
        "build",
        "package.json",
        "yarn.lock",
        "README.md",
        "LICENSE.txt"
    ],
    "dependencies": {
        "@formio/core": "2.0.0-rc.8",
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "formiojs": "^4.18.0",
        "isolated-vm": "^4.6.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "nunjucks": "^3.2.4",
        "nunjucks-date-filter": "^0.1.1"
    },
    "scripts": {
        "test": "mocha -r ts-node/register -b -t 0 'src/**/__tests__/*.test.ts'",
        "start": "DOTENV_CONFIG_PATH=./.env node build/server.js",
        "clean": "rm -rf build",
        "copy-html": "rsync -avm --include='*.html' -f 'hide,! */' ./src/ ./build/",
        "build": "npm run clean && tsc && npm run copy-html",
        "prepack": "npm run build && rm -rf build/server && rm -rf build/core/__tests__",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/chai": "^4.3.11",
        "@types/debug": "^4.1.10",
        "@types/express": "^4.17.20",
        "@types/lodash": "^4.14.200",
        "@types/method-override": "^0.0.35",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.8.9",
        "@types/nunjucks": "^3.2.6",
        "chai": "^4.3.10",
        "eslint": ">=7.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
        "eslint-plugin-promise": "^6.0.0",
        "husky": ">=6",
        "lint-staged": ">=10",
        "mocha": "^10.2.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.1.0",
        "ts-node": "^10.9.1",
        "tsc": "^2.0.4",
        "typescript": "^5.2.2"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "eslint --fix"
        ],
        "*.{ts,json,md}": "prettier --write"
    }
}
