{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-prototype-builtins": "off",
        "@typescript-eslint/no-explicit-any": "warn"
    },
    "root": true,
    "ignorePatterns": ["build/", "node_modules/"]
}
